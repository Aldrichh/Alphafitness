<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="101dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="37dp"
        android:layout_marginLeft="37dp"
        android:layout_marginTop="7dp"
        app:srcCompat="@drawable/profileicon" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView4"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:textColor="@color/colorBlack"
        android:textSize="18dp"
        android:text="Gender" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="229dp"
        android:layout_marginLeft="229dp"
        android:layout_marginTop="108dp"
        android:text="Weight"
        android:textColor="@color/colorBlack"
        android:textSize="18dp" />

    <EditText
        android:id="@+id/nameText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        android:background="@color/lightGray"
        android:textSize="20dp"
        android:ems="10"
        android:inputType="textPersonName"
         />

    <TextView
        android:id="@+id/background"
        android:layout_width="match_parent"
        android:layout_height="470dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="138dp"
        android:background="@color/lightGray" />

    <TextView
        android:id="@+id/Average-Weekly"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:layout_alignStart="@+id/textView7"
        android:layout_alignLeft="@+id/textView7"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-11dp"
        android:layout_marginLeft="-11dp"
        android:layout_marginTop="140dp"
        android:text="Average/Weekly"
        android:textColor="@color/colorBlack"
        android:textSize="22dp" />

    <TextView
        android:id="@+id/AllTime"
        android:layout_width="200dp"
        android:layout_height="35dp"
        android:layout_alignStart="@+id/textView7"
        android:layout_alignLeft="@+id/textView7"
        android:layout_below="@id/CaloriesHeader"
        android:layout_marginTop="15dp"
        android:text="All Time"
        android:layout_marginLeft="-11dp"
        android:layout_marginStart="-11dp"
        android:textColor="@color/colorBlack"
        android:textSize="22dp" />

    <TextView
        android:id="@+id/firstWhiteBox"
        android:layout_width="385dp"
        android:layout_height="45dp"
        android:background="@color/colorWhite"
        android:layout_below="@+id/Average-Weekly"
        android:layout_alignStart="@+id/Average-Weekly"
        android:layout_alignLeft="@+id/Average-Weekly"
         />

    <TextView
        android:id="@+id/secondWhiteBox"
        android:layout_width="385dp"
        android:layout_height="45dp"
        android:background="@color/colorWhite"
        android:layout_below="@+id/firstWhiteBox"
        android:layout_marginTop="3dp"
        android:layout_alignLeft="@+id/Average-Weekly"
        />

    <TextView
        android:id="@+id/thirdWhiteBox"
        android:layout_width="385dp"
        android:layout_height="45dp"
        android:background="@color/colorWhite"
        android:layout_below="@+id/secondWhiteBox"
        android:layout_marginTop="3dp"
        android:layout_alignLeft="@+id/Average-Weekly"
        />


    <TextView
        android:id="@+id/fourthWhiteBox"
        android:layout_width="385dp"
        android:layout_height="45dp"
        android:background="@color/colorWhite"
        android:layout_below="@+id/thirdWhiteBox"
        android:layout_marginTop="3dp"
        android:layout_alignLeft="@+id/Average-Weekly"
        />

    <TextView
        android:id="@+id/fifthWhiteBox"
        android:layout_width="385dp"
        android:layout_height="45dp"
        android:background="@color/colorWhite"
        android:layout_below="@+id/fourthWhiteBox"
        android:layout_marginTop="43dp"
        android:layout_alignLeft="@+id/Average-Weekly"
        />

    <TextView
        android:id="@+id/sixthWhiteBox"
        android:layout_width="385dp"
        android:layout_height="45dp"
        android:background="@color/colorWhite"
        android:layout_below="@+id/fifthWhiteBox"
        android:layout_marginTop="3dp"
        android:layout_alignLeft="@+id/Average-Weekly"
        />

    <TextView
        android:id="@+id/seventhWhiteBox"
        android:layout_width="385dp"
        android:layout_height="45dp"
        android:background="@color/colorWhite"
        android:layout_below="@+id/sixthWhiteBox"
        android:layout_marginTop="3dp"
        android:layout_alignLeft="@+id/Average-Weekly"
        />

    <TextView
        android:id="@+id/eighthWhiteBox"
        android:layout_width="385dp"
        android:layout_height="45dp"
        android:background="@color/colorWhite"
        android:layout_below="@+id/seventhWhiteBox"
        android:layout_marginTop="3dp"
        android:layout_alignLeft="@+id/Average-Weekly"
        />



    <TextView
        android:id="@+id/DistanceHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView7"
        android:layout_alignParentTop="true"
        android:layout_marginTop="185dp"
        android:layout_marginEnd="-23dp"
        android:text="Distance"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/timeHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@id/DistanceHeader"
        android:layout_marginTop="20dp"
        android:text="Time"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/WorkoutsHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@id/timeHeader"
        android:layout_marginTop="20dp"
        android:text="Workouts"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/CaloriesHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@id/WorkoutsHeader"
        android:layout_marginTop="20dp"
        android:text="Calories"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />


    <TextView
        android:id="@+id/DistanceHeader2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@id/CaloriesHeader"
        android:layout_marginTop="63dp"
        android:text="Distance"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/timeHeader2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@id/DistanceHeader2"
        android:layout_marginTop="20dp"
        android:text="Time"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/WorkoutsHeader2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@id/timeHeader2"
        android:layout_marginTop="20dp"
        android:text="Workouts"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/CaloriesHeader2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@id/WorkoutsHeader2"
        android:layout_marginTop="20dp"
        android:text="Calories"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />

    <EditText
        android:id="@+id/genderText"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView8"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="98dp"
        android:layout_marginLeft="98dp"
        android:layout_marginTop="0dp"
        android:background="@color/lightGray"
        android:textColor="@color/colorBlack"
        android:textSize="18dp"
         />

    <EditText
        android:id="@+id/weightText"
        android:background="@color/lightGray"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView4"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:textSize="18dp"
        android:textColor="@color/colorBlack"
         />

    <Button
        android:id="@+id/updateButton"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:textSize="12dp"
        android:layout_alignBaseline="@+id/Average-Weekly"
        android:layout_alignEnd="@+id/weightText"
        android:layout_alignRight="@+id/weightText"
        android:text="Update" />

    <TextView
        android:id="@+id/distanceTV"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/DistanceHeader"
        android:layout_marginStart="-18dp"
        android:layout_marginLeft="-18dp"
        android:layout_marginTop="0dp"
        android:layout_toEndOf="@+id/textView8"
        android:layout_toRightOf="@+id/textView8"
        android:gravity="end"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/timeTV"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/distanceTV"
        android:layout_alignTop="@+id/timeHeader"
        android:textColor="@color/colorBlack"
        android:textSize="20dp"
        android:gravity="end"
         />

    <TextView
        android:id="@+id/workoutsTV"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/distanceTV"
        android:layout_alignTop="@+id/WorkoutsHeader"
        android:gravity="end"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/caloriesTV"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/distanceTV"
        android:layout_alignTop="@+id/CaloriesHeader"
        android:gravity="end"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/allTimeDisTV"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/distanceTV"
        android:layout_alignTop="@+id/DistanceHeader2"
        android:gravity="end"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/allTimeTV"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/distanceTV"
        android:layout_alignTop="@+id/timeHeader2"
        android:gravity="end"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/allTimeWorkoutsTV"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/distanceTV"
        android:layout_alignTop="@+id/WorkoutsHeader2"
        android:gravity="end"
        android:text="TextView"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/allTimeCalsTV"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/distanceTV"
        android:layout_alignTop="@+id/CaloriesHeader2"
        android:gravity="end"
        android:text="TextView"
        android:textColor="@color/colorBlack"
        android:textSize="20dp" />


</RelativeLayout>